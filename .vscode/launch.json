	"version": "0.2.0",
	"configurations": [
		{
			"request": "launch",
			"name": "Lauch Invoice CLI",
			"type": "pwa-node",
			"program": "${workspaceFolder}/mod.ts",
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "deno",
			"runtimeArgs": [
				"run",
				"--unstable",
				"--import-map",
				"./imports.json",
				"--inspect",
				"--allow-all"
			],
			"args": ["invoice"],
			"attachSimplePort": 9229
		}
	]
}
